        "\t\trequestInput: {",
        "\t\t\tuuid: $data.request.uuid !== undefined ? $data.request.uuid : null,",
        "\t\t\titem: $data.request.item !== undefined ? $data.request.item : null,",
        "\t\t\tquantity: $data.request.quantity !== undefined ? $data.request.quantity : null,",
        "\t\t\tprice: $data.request.price !== undefined ? $data.request.price : null,",
